{
  "name": "battleforthenet",
  "version": "3.0.0",
  "description": "Battle for the Net website",
  "author": "",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "pregenerate": "npm run generate-strings && npm run generate-routes && npm run generate-media",
    "postgenerate": "node scripts/patch-redalert.js",
    "generate-strings": "node scripts/generate-strings.js",
    "generate-routes": "node scripts/generate-routes.js",
    "generate-media": "node scripts/generate-media.js",
    "deploy": "aws s3 sync dist s3://www.battleforthenet.com --delete",
    "deploy-branch": "bash scripts/deploy-branch.sh",
    "analyze": "nuxt build --analyze",
    "serve-static": "npm run generate && static dist"
  },
  "dependencies": {
    "leaflet": "^1.3.1",
    "nuxt": "^1.0.0"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-eslint": "^8.2.1",
    "babel-polyfill": "^6.26.0",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "haversine": "^1.1.0",
    "js-yaml": "^3.11.0",
    "lodash": "^4.17.5",
    "node-sass": "^4.9.2",
    "normalize.css": "^8.0.0",
    "nuxt-sass-resources-loader": "^2.0.3",
    "qs": "^6.5.2",
    "raven-js": "^3.24.2",
    "sass-loader": "^7.0.3",
    "showdown": "^1.8.6",
    "vue-count-to": "^1.0.13",
    "vue-i18n": "^7.6.0",
    "yaml-loader": "^0.5.0"
  }
}
